    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.54.0",
    "postcss": "^8.5.6",
    "prisma": "^6.17.1",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.7.0",
